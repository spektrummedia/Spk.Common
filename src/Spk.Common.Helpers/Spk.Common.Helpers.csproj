<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>net45;netstandard2.0;netstandard2.1</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>spektrummedia</Authors>
        <Company>Spektrum Media</Company>
        <PackageIconUrl>https://avatars1.githubusercontent.com/u/13052913</PackageIconUrl>
        <RepositoryUrl>https://github.com/spektrummedia/Spk.Common</RepositoryUrl>
        <Version>1.12.0</Version>
        <Description>Library exposing code used in multiple projects.</Description>
    </PropertyGroup>
    <PropertyGroup>
      <DebugType>full</DebugType>
      <DebugSymbols>true</DebugSymbols>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="JetBrains.Annotations" Version="2019.1.3" />
    </ItemGroup>
</Project>
